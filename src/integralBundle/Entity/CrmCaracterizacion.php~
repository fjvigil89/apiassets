     *
     * @param string $porcComparar
     * @return CrmCaracterizacion
     */
    public function setPorcComparar($porcComparar)
    {
        $this->porcComparar = $porcComparar;

        return $this;
    }

    /**
     * Get porcComparar
     *
     * @return string 
     */
    public function getPorcComparar()
    {
        return $this->porcComparar;
    }

