<?php

namespace integralBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Pvxd06detalleprog
 *
 * @ORM\Table(name="PVXD_06DetalleProg")
 * @ORM\Entity
 */
class Pvxd06detalleprog
{
    /**
     * @var integer
     *
     * @ORM\Column(name="Contador", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $contador;

    /**
     * @var integer
     *
     * @ORM\Column(name="Clase", type="smallint", nullable=false)
     */
    private $clase;

    /**
     * @var integer
     *
     * @ORM\Column(name="Ida", type="integer", nullable=false)
     */
    private $ida;

    /**
     * @var integer
     *
     * @ORM\Column(name="Regreso", type="integer", nullable=false)
     */
    private $regreso;



    /**
     * Get contador
     *
     * @return integer 
     */
    public function getContador()
    {
        return $this->contador;
    }

    /**
     * Set clase
     *
     * @param integer $clase
     * @return Pvxd06detalleprog
     */
    public function setClase($clase)
    {
        $this->clase = $clase;

        return $this;
    }

    /**
     * Get clase
     *
     * @return integer 
     */
    public function getClase()
    {
        return $this->clase;
    }

    /**
     * Set ida
     *
     * @param integer $ida
     * @return Pvxd06detalleprog
     */
    public function setIda($ida)
    {
        $this->ida = $ida;

        return $this;
    }

    /**
     * Get ida
     *
     * @return integer 
     */
    public function getIda()
    {
        return $this->ida;
    }

    /**
     * Set regreso
     *
     * @param integer $regreso
     * @return Pvxd06detalleprog
     */
    public function setRegreso($regreso)
    {
        $this->regreso = $regreso;

        return $this;
    }

    /**
     * Get regreso
     *
     * @return integer 
     */
    public function getRegreso()
    {
        return $this->regreso;
    }
}
